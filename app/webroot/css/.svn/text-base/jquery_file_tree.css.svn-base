UL.jqueryFileTree {
	font-family: "Lucida Grande", Verdana, sans-serif;
	font-size: 11px;
	line-height: 18px;
	padding: 0px;
	margin: 0px;
}

UL.jqueryFileTree LI {
	list-style: none;
	padding: 0px;
	padding-left: 20px;
	margin: 0px;
	white-space: nowrap;
}

UL.jqueryFileTree A {
	color: #333;
	text-decoration: none;
	display: block;
	padding: 0px 2px;
}

UL.jqueryFileTree A:hover {
	background: #BDF;
}

/* Core Styles */
.jqueryFileTree LI.directory { background: url(/css/images/directory.png) left top no-repeat; }
.jqueryFileTree LI.expanded { background: url(/css/images/folder_open.png) left top no-repeat; }
.jqueryFileTree LI.file { background: url(/css/images/file.png) left top no-repeat; }
.jqueryFileTree LI.wait { background: url(/css/images/spinner.gif) left top no-repeat; }
